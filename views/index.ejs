<!DOCTYPE html>
<html>
    <head>
      <%- include('partial/linkData') %>
    </head>

    <body>

      <% contentHeader.forEach(function(contentHeaderItem){ %>
        <%- include(contentHeaderItem.view, contentHeaderItem.data) %>
      <% }) %>

      <% contentMenu.forEach(function(contentMenuItem){ %>
        <%- include(contentMenuItem.view, contentMenuItem.data) %>
      <% }) %>

      <h1 style="color: crimson;margin: 10px 50px;text-align: center;"><%= tb %></h1> 
      
      <% contentSlider.forEach(function(contentSlider){ %>
        <%- include(contentSlider.view, contentSlider.data) %>
      <% }) %>

      <% content.forEach(function(contentItem){ %>
        <%- include(contentItem.view, contentItem.data) %>
      <% }) %>
      

      <%- include('partial/footer') %>
              
    </body>
    <script src="/js/home.js"></script>
    <script src="/js/menu.js"></script>
</html>